<div class="page-header">
  <%= link_to members_path, class: 'btn btn-default' do %>
    All Members
  <% end %>
  <%= link_to edit_member_path(@member), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show member</h1>
</div>

<dl class="dl-horizontal">
  <dt>User:</dt>
  <dd><%= @member.user %></dd>
  <dt>Tenant:</dt>
  <dd><%= @member.tenant %></dd>
</dl>
